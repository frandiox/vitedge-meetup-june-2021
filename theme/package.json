{
  "name": "slidev-theme-light-icons",
  "version": "1.0.2",
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Pulkit Aggarwal<pulkit.cloud@gmail.com>",
  "repository": {
    "type": "git",
